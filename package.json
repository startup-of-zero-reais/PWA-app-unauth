{
  "name": "family-finances",
  "version": "0.1.0",
  "private": true,
  "description": "Family Finances Aplicativo PWA para Finan√ßas Familiar",
  "author": "Vicente Nascimento",
  "scripts": {
    "dev": "next dev ",
    "build": "next build ",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test -- --watch",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test -- --coverage",
    "test:coveralls": "yarn run test:ci && coveralls < coverage/lcov.info",
    "start": "next start",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:8080 test:cypress:run"
  },
  "dependencies": {
    "next": "11.1.0",
    "next-pwa": "^5.2.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.9.0",
    "@testing-library/jest-dom": "^5.13.0",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "cross-env": "^7.0.3",
    "cypress": "^7.5.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "faker": "^5.5.3",
    "full-icu": "^1.3.4",
    "jest": "^27.0.4",
    "jest-localstorage-mock": "^2.4.14",
    "prettier": "^2.3.1",
    "start-server-and-test": "^1.12.5",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.2"
  }
}
